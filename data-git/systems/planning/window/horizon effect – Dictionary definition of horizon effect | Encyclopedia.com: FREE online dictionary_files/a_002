Search(){$("#rcFlyout").fadeOut();$("html").toggleClass("search-open").removeClass("flyout-open").css("border-right","0")}function closeOverlays(){var a=$("html");$("#rcFlyout").fadeOut();a.removeClass("flyout-open").removeClass("search-open").css("border-right","0")}function showBackToTop(){if($(window).scrollTop()>50){$(".back-to-top").addClass("visible")}else{if($(window).scrollTop()<=500){$(".back-to-top").removeClass("visible")}}}window.onload=init();